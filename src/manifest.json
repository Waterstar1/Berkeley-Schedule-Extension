{
  "name": "Everything Pikachu",
  "version": "1.0",
  "manifest_version": 2,
  "browser_action": {
    "default_icon": "pokeball.png",
    "default_title": "Lookup a definition!"
  },
  "permissions": [
    "webRequest",
    "https://solr-aws-elb-production.ratemyprofessors.com/*",
    "https://classes.berkeley.edu/search/class/*"
  ],
  "content_scripts": [
    {
      "matches": ["https://classes.berkeley.edu/search/class/*"],
      "css": ["sheet.css"],
      "js": ["jquery-3.5.1.js", "content.js"]
    }
  ],
  "background": {
    "scripts": ["background.js"]
  }
}